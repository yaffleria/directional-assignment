{
  "openapi": "3.0.3",
  "info": {
    "title": "FE Hiring REST API",
    "version": "1.0.0",
    "description": "로그인(JWT) / 본인 포스트 작성·조회 / 목업 300건 제공 - Enum/조건부 입력/제한조건 반영"
  },
  "servers": [
    {
      "url": "https://fe-hiring-rest-api.vercel.app",
      "description": "Vercel"
    },
    {
      "url": "http://localhost:4000",
      "description": "Local"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "Category": {
        "type": "string",
        "enum": [
          "NOTICE",
          "QNA",
          "FREE"
        ]
      },
      "SortField": {
        "type": "string",
        "enum": [
          "createdAt",
          "title"
        ]
      },
      "SortOrder": {
        "type": "string",
        "enum": [
          "asc",
          "desc"
        ]
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "u_1"
          },
          "email": {
            "type": "string",
            "example": "alice@example.com"
          }
        }
      },
      "Post": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "p_abc123"
          },
          "userId": {
            "type": "string",
            "example": "u_1"
          },
          "title": {
            "type": "string",
            "example": "Sample Post #1"
          },
          "body": {
            "type": "string",
            "example": "Hello world"
          },
          "category": {
            "$ref": "#/components/schemas/Category"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": [
              "react",
              "ts"
            ]
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "id",
          "userId",
          "title",
          "body",
          "category",
          "tags",
          "createdAt"
        ]
      },
      "LoginRequest": {
        "type": "object",
        "required": [
          "email",
          "password"
        ],
        "properties": {
          "email": {
            "type": "string",
            "example": "alice@example.com"
          },
          "password": {
            "type": "string",
            "example": "alice1234"
          }
        }
      },
      "LoginResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          }
        }
      },
      "PostCreateRequest": {
        "type": "object",
        "required": [
          "title",
          "body",
          "category"
        ],
        "properties": {
          "title": {
            "type": "string",
            "maxLength": 80
          },
          "body": {
            "type": "string",
            "maxLength": 2000
          },
          "category": {
            "$ref": "#/components/schemas/Category"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string",
              "maxLength": 24
            },
            "maxItems": 5
          }
        }
      },
      "PostUpdateRequest": {
        "description": "부분 업데이트. 최소 1개 이상의 필드를 포함.",
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "maxLength": 80
          },
          "body": {
            "type": "string",
            "maxLength": 2000
          },
          "category": {
            "$ref": "#/components/schemas/Category"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string",
              "maxLength": 24
            },
            "maxItems": 5
          }
        },
        "additionalProperties": false
      },
      "PostListResponse": {
        "type": "object",
        "properties": {
          "items": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Post"
            }
          },
          "nextCursor": {
            "type": "string",
            "nullable": true,
            "description": "다음 페이지를 위한 커서 토큰(opaque base64url). 더 이상 페이지가 없으면 null.",
            "example": "eyJzIjoidGl0bGUiLCJvIjoiYXNjIiwidiI6Iu2VnO2VgCIsImlkIjoicF83ODkifQ"
          },
          "prevCursor": {
            "type": "string",
            "nullable": true,
            "description": "이전 페이지를 위한 커서 토큰(opaque base64url). 더 이상 페이지가 없으면 null.",
            "example": "eyJzIjoidGl0bGUiLCJvIjoiYXNjIiwidiI6Iu2VnO2VgCIsImlkIjoicF83ODkifQ"
          }
        }
      },
      "CoffeeDataPoint": {
        "type": "object",
        "properties": {
          "cups": {
            "type": "integer",
            "example": 3,
            "minimum": 0
          },
          "bugs": {
            "type": "integer",
            "example": 6,
            "minimum": 0
          },
          "productivity": {
            "type": "integer",
            "example": 85,
            "minimum": 0,
            "maximum": 100
          }
        },
        "required": [
          "cups",
          "bugs",
          "productivity"
        ]
      },
      "CoffeeTeam": {
        "type": "object",
        "properties": {
          "team": {
            "type": "string",
            "example": "Frontend"
          },
          "series": {
            "type": "array",
            "items": {
