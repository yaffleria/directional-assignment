    "parameters": {
      "LimitParam": {
        "name": "limit",
        "in": "query",
        "schema": {
          "type": "integer",
          "minimum": 1,
          "maximum": 100,
          "default": 10
        },
        "description": "페이지 크기 (1~100)"
      },
      "NextCursorParam": {
        "name": "nextCursor",
        "in": "query",
        "schema": {
          "type": "string"
        },
        "description": "다음 페이지용 커서(opaque). 이전 응답의 nextCursor를 그대로 전달. prevCursor와 동시 사용 불가."
      },
      "PrevCursorParam": {
        "name": "prevCursor",
        "in": "query",
        "schema": {
          "type": "string"
        },
        "description": "이전 페이지용 커서(opaque). 이전 응답의 prevCursor를 그대로 전달. nextCursor와 동시 사용 불가."
      },
      "SortParam": {
        "name": "sort",
        "in": "query",
        "schema": {
          "$ref": "#/components/schemas/SortField"
        },
        "description": "정렬 필드"
      },
      "OrderParam": {
        "name": "order",
        "in": "query",
        "schema": {
          "$ref": "#/components/schemas/SortOrder"
        },
        "description": "정렬 방향"
      },
      "CategoryParam": {
        "name": "category",
        "in": "query",
        "schema": {
          "$ref": "#/components/schemas/Category"
        }
      },
      "FromParam": {
        "name": "from",
        "in": "query",
        "schema": {
          "type": "string",
          "format": "date-time"
        }
      },
      "ToParam": {
        "name": "to",
        "in": "query",
        "schema": {
          "type": "string",
          "format": "date-time"
        }
      },
      "IdPath": {
        "name": "id",
        "in": "path",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "SearchParam": {
        "name": "search",
        "in": "query",
        "schema": {
          "type": "string"
        },
        "description": "제목/본문 검색어 (공백으로 여러 단어 입력 시 AND 매칭)"
      },
      "CountParam": {
        "name": "count",
        "in": "query",
        "schema": {
          "type": "integer",
          "default": 300,
          "minimum": 1,
          "maximum": 500
        },
        "description": "반환할 항목 수 (고정 500개 중 앞에서부터 slice). 기본 300, 최대 500",
        "example": 5
      }
    }
  },
  "paths": {
    "/health": {
      "get": {
        "summary": "Health check",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "summary": "로그인 (JWT 발급)",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "로그인 성공",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid payload"
          },
          "401": {
            "description": "Invalid credentials"
          }
        }
      }
    },
    "/posts": {
      "get": {
        "summary": "내 포스트 목록 (본인 것만)",
        "description": "양방향 커서 기반 페이지네이션(prevCursor/nextCursor)을 지원합니다. prev/next를 동시에 전달할 수 없습니다.",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/LimitParam"
          },
          {
            "$ref": "#/components/parameters/PrevCursorParam"
          },
          {
            "$ref": "#/components/parameters/NextCursorParam"
          },
          {
            "$ref": "#/components/parameters/SortParam"
          },
          {
            "$ref": "#/components/parameters/OrderParam"
          },
          {
            "$ref": "#/components/parameters/CategoryParam"
          },
          {
            "$ref": "#/components/parameters/FromParam"
          },
          {
            "$ref": "#/components/parameters/ToParam"
          },
          {
            "$ref": "#/components/parameters/SearchParam"
          }
        ],
        "responses": {
          "200": {
            "description": "목록",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PostListResponse"
                }
              }
            }
          },
          "400": {
            "description": "Invalid query"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      },
      "post": {
        "summary": "포스트 작성 (본인)",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PostCreateRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "생성됨",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Post"
                }
              }
            }
          },
          "400": {
            "description": "Invalid payload"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      },
      "delete": {
        "summary": "내 모든 포스트 삭제",
        "description": "JWT 소유자의 모든 포스트를 삭제합니다.",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "삭제된 개수 반환",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DeleteResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/posts/{id}": {
      "get": {
