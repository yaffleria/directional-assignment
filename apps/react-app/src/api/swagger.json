      "TopCoffeeBrandItem": {
        "type": "object",
        "properties": {
          "brand": {
            "type": "string",
            "example": "스타벅스"
          },
          "popularity": {
            "type": "integer",
            "example": 40,
            "minimum": 0,
            "maximum": 100
          }
        },
        "required": [
          "brand",
          "popularity"
        ]
      },
      "TopCoffeeBrandsResponse": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/TopCoffeeBrandItem"
        },
