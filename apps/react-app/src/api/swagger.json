      "WeeklyMoodItem": {
        "type": "object",
        "properties": {
          "week": {
            "type": "string",
            "example": "2024-12-09"
          },
          "happy": {
            "type": "integer",
            "example": 72,
            "minimum": 0,
            "maximum": 100
