      "WeeklyWorkoutItem": {
        "type": "object",
        "properties": {
          "week": {
            "type": "string",
            "example": "2025-01-05"
          },
          "running": {
            "type": "integer",
            "example": 12,
            "minimum": 0
          },
          "cycling": {
            "type": "integer",
            "example": 8,
            "minimum": 0
          },
          "stretching": {
            "type": "integer",
            "example": 6,
            "minimum": 0
          }
        },
        "required": [
          "week",
          "running",
          "cycling",
          "stretching"
        ]
      },
      "WeeklyWorkoutTrendResponse": {
