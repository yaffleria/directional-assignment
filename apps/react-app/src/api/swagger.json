      "DeleteResponse": {
        "type": "object",
        "properties": {
          "ok": {
            "type": "boolean",
            "example": true
          },
          "deleted": {
            "type": "integer",
            "minimum": 0,
            "example": 1
          }
        },
        "required": [
          "ok",
          "deleted"
        ]
      },
      "SnackImpactDataPoint": {
        "type": "object",
